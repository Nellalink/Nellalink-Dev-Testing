class NellalinkDevTesting
{

    public function __construct()
    {
        add_shortcode('nellalink-dev-testing', array($this, 'nellalink_test'));
    }

    public function nellalink_test() {
        ob_start();
        $RimplenetAuthorizationObj = new RimplenetAuthorization();
        var_dump($RimplenetAuthorizationObj->authorization("eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..TX-xxGCl5JMn3IUu8bBdk3vAJr-FI5UQfOd_pztX0Vw"));
        
        return ob_get_clean();
    }
}

$NellalinkDevTesting = new NellalinkDevTesting();
